/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    
    // My implementation
    Nand(a=a, b=b, out=result);
    Nand(a=a, b=result, out=c1);
    Nand(a=result, b=b, out=c2);
    Nand(a=c1, b=c2, out=out);

    // 2nd implementation:
    // Not(in=a,out=na);
