	  // And(a=na,b=b,out=c);
	  // And(a=a,b=nb,out=d);
	  // Or(a=c,b=d,out=out);
   
   
    // 3rd impementation: 
    // Or(a=a, b=b, out=c1);
    // Nand(a=a, b=b, out=c2);
    // And(a=c1, b=c2, out=out);
    
    // Explanations of each chip and credit for 2nd and 3rd implementation is in the description of this file
